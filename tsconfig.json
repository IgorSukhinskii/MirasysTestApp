{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES6", // Requires ES6 and above
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./assets/*"]
    },
    "plugins": [{
      "name": "@0no-co/graphqlsp",
      "schema": "./schema.graphql"
    }],
    "types": ["jest"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
